{
	"$schema": "https://json.schemastore.org/tsconfig",
	"ts-node": {
		"esm": true,
		"transpileOnly": true,
		"files": true,
		"compilerOptions": {
			"target": "ES2022",
			"lib": ["ES2022"],
			"module": "Node16",
			"moduleResolution": "node16",
			"incremental": true,
			"noImplicitAny": false,
			"esModuleInterop": true,
			"skipLibCheck": true,
			"allowJs": true,
			"checkJs": true
		}
	}
}
